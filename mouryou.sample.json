{
  "develop_log_level": 0,
  "timeout": 1,
  "sleep": 30,
  "wait": 30,
  "margin": 0.007,
  "algorithm": "basic_spike",
  "use_hetero": false,
  "adjust_server_num": true,
  "origin_machine_names": [
    "origin-01",
    "origin-02"
  ],
  "always_running_machines": [
    "mirror-01",
    "mirror-02"
  ],
  "start_machine_ids": [1, 2],
  "web_socket": {
    "origin": "http://localhost/",
    "url": "ws://localhost:8000/ws"
  },
  "cluster": {
    "load_balancer": {
      "name": "haproxy",
      "virtual_ip": "192.168.11.11",
      "algorithm": "wlc",
      "threshold_out": 0.8,
      "threshold_in": 0.5,
      "margin": 0.05,
      "scale_out": 2,
      "scale_in": 6,
      "diff": 0.2
    },
    "hypervisors": [
      {
        "name": "azure",
        "virtual_machines": {
          "web-server-01": {
            "id": 1,
            "name": "web-server-01",
            "host": "192.168.11.01",
            "unit_time": 30,
            "unit_cost": 10,
            "average": 250,
            "weight": 10
          },
          "web-server-02": {
            "id": 2,
            "name": "web-server-02",
            "host": "192.168.11.02",
            "unit_time": 30,
            "unit_cost": 10,
            "average": 250,
            "weight": 10
          }
        }
      }
    ]
  }
}